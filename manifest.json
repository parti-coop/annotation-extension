{
  "manifest_version": 3,
  "name": "Mul-Mm-Pyo",
  "description": "Chrome Extension for Web Annotation",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}